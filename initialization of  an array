#include<stdio.h>
int main()
{
	int n,i,j;
	int array[n];
	printf("Enter the number of element in an array:\n");
	scanf("%d",&n);
	printf("Enter the element in an array:\n");
	scanf("%d",&array[n]);
    for(i=0;i<=n;i++)
    {
    	scanf("%d",&array[i]);
	}
	printf("Array element are");
	for(j=0;j<=n;j++)
	{
		printf("%d\n",&array[j]);
	}
}
